<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Text?>

<GridPane alignment="CENTER" hgap="10" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="143.0" prefWidth="310.0" snapToPixel="false" vgap="10"
	xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.LoadGameDialogController">
	<children>
		<ComboBox fx:id="loadItem" prefWidth="150.0" promptText="Válassz"
			GridPane.columnIndex="1">
		</ComboBox>
		<Text fontSmoothingType="LCD" strokeType="OUTSIDE" strokeWidth="0.0"
			text="Válassz egy mentett állást: " wrappingWidth="96.783203125" />
		<Button fx:id="loadSavedGame" mnemonicParsing="false"
			onAction="#onLoadGameAction" text="Betöltés" GridPane.columnIndex="1"
			GridPane.rowIndex="2" />
		<Button fx:id="back" alignment="CENTER" contentDisplay="CENTER"
			mnemonicParsing="false" onAction="#onBackButtonAction" text="Vissza"
			textAlignment="CENTER" GridPane.rowIndex="2" />
	</children>
</GridPane>
